class stataxlx {
	string fpath
	statalist title
	array sheets
	double isheet = 0
	double n = 0
}
program .set
	args x y z
	
	.fpath = "`x'"
	.title  = `y'.copy
	
	tempname slist
	.`slist' = .Global`z'.copy

	while ( `.`slist'.loop_next' ) {
		.newsheet`.n' = .statasheet.new
		.newsheet`.n'.set "`.`slist'.loop_get'"
		.sheets.Arrpush .newsheet`.n'
		.n = `.n' + 1
	}
end
program .create_contents_sheet
	
	
end
program .get_label
	args i
	
	class exit `.sheets[`i']'.lbl
end
